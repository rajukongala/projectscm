pipeline {
    agent any
    stages {
        stage('test') {
            steps {
                echo "Hello World test"
                bat "mvn test"
            }
        }
        stage('compile') {
            steps {
                echo "Hello World compile"
                bat "mvn compile"
            }
        }
        stage('deploy') {
            steps {
                echo "Hello World deploy"
            }
        }
    }
    
        }
